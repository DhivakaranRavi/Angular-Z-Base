{
  "name": "angular-base",
  "version": "1.0.0",
  "description": "Base Setup For Angular Application",
  "scripts": {
    "precommit": "lint-staged",
    "start": "webpack-dev-server  --config ./config/webpack.dev.js --open",
    "build": "webpack --config ./config/webpack.prod.js",
